<table id='mytable' style="border: 1px solid black">
	<tr>
		<th>one</th>
		<th>two</th>
	</tr>
	<tr>
		<td>100</td>
		<td>200</td>
	</tr>
	<tr>
		<td>300</td>
		<td>400</td>
	</tr>
</table>
<br>
<p id='rows'></p>
<br>
<p id='columns'></p>
<br>
<p id='first_cell_value'></p>
<button onclick="insert_row('mytable')">count rows</button>
<script type="text/javascript">
	function insert_row(table){	
		let mytableElement = document.getElementById('mytable');	
		//inserting row at the bottom
		let row = mytableElement.insertRow(mytableElement.rows.length);
		let cell1 = row.insertCell(0);
		let cell2 = row.insertCell(1);
		cell1.innerHTML = 999;
		cell2.innerHTML = 888;
	}
</script>
